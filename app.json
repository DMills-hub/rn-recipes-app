{
  "expo": {
    "name": "Recipe & Me",
    "slug": "recipe-and-me",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#B0E0E6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.myrecipes.recipe-me",
      "buildNumber": "1.1.1",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to save images of there recipes that they have created."
      }
    },
    "android": {
      "package": "com.myrecipes.recipeme",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_icon.png",
        "backgroundColor": "#B0E0E6"
      },
      "permissions": [
        "CAMERA"
      ]
    }
  }
}
